startCodon ="ATG"
stopCodod = "TAA"
DNA = ""

def returnGene(DNA,start,stop):
    if(len(DNA) < 6):
        return ""
    str = DNA.upper()
    if(len(str[start:stop+3])%3 == 0):
        return str[start:stop+3]
    return ""
       
def findStart(str, startCodon):
    str = str.upper()
    for i in range(0,len(str)):
        if(str[i:i+3] == startCodon):
            return i
        
def findStop(str, stopCodon):
    str = str.upper()
    for i in range(0,len(str)):
        if(str[i:i+3] == stopCodon):
            return i+2
    return -1 
    

def findSS(str):
    start = findStart(str, startCodon)
    if start >= 0:
        stop = findStop(str[start+2:],stopCodod)+start
        if stop > 0:
            return [start,stop]
    return []


def findGene(str):
    genes = []
    while(True):
        if len(findSS(str))>0:          
            if(returnGene(str,findSS(str)[0],findSS(str)[1]) != ""):
                genes.append(returnGene(str,findSS(str)[0],findSS(str)[1]))
                str = str[findSS(str)[0]+1:]
            else:
                str = str[findSS(str)[0]+1:]           
        else:
            break
    return genes   

for i in findGene(DNA):
    print i
